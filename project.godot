[application]

config/name="Blades in the Dark"
config/description="A program to help remotely play Blades in the Dark tabletop roleplaying game."
run/main_scene="res://start_screen/StartScreen.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="BITD"
config/icon="res://Shared/Art/Icons/icon.png"
config/version="v0.65"

[autoload]

